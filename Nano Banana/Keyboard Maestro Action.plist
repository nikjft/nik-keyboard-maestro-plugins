<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Name</key>
	<string>Nano Banana Image Generation</string>
	<key>Author</key>
	<string>Nik</string>
	<key>Icon</key>
	<string>icon.png</string>
	<key>Help</key>
	<string>Uses Gemini API to generate images - get an API key at https://aistudio.google.com/api-keys</string>
	<key>URL</key>
	<string>https://github.com/nikjft/nik-keyboard-maestro-plugins</string>
	<key>Script</key>
	<string>imagen_request.sh</string>
	<key>Title</key>
	<string>Nano Banana</string>
	<key>Results</key>
	<string>Window|Variable|Clipboard|File</string>
	<key>Parameters</key>
	<array>
		<dict>
			<key>Label</key>
			<string>Model</string>
			<key>Type</key>
			<string>PopupMenu</string>
			<key>Default</key>
			<string>gemini-2.5-flash-image</string>
			<key>Menu</key>
			<string>gemini-2.5-flash-image|gemini-3-pro-image-preview-2K|gemini-3-pro-image-preview-4K</string>
		</dict>
		<dict>
			<key>Label</key>
			<string>API Key</string>
			<key>Type</key>
			<string>TokenString</string>
			<key>Default</key>
			<string>%Variable%GeminiAPIKey%</string>
			<key>Placeholder</key>
			<string>Use %Variable%GeminiAPIKey%</string>
		</dict>
		<dict>
			<key>Label</key>
			<string>Prompt</string>
			<key>Type</key>
			<string>TokenText</string>
			<key>Default</key>
			<string></string>
			<key>Placeholder</key>
			<string>Describe the image you want to generate...</string>
		</dict>
		<dict>
			<key>Label</key>
			<string>Aspect Ratio</string>
			<key>Type</key>
			<string>PopupMenu</string>
			<key>Default</key>
			<string>1:1</string>
			<key>Menu</key>
			<string>1:1|16:9|4:3|3:4|9:16</string>
		</dict>
	</array>
</dict>
</plist>